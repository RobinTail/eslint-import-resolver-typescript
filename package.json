{
  "name": "@robintail/import-resolver",
  "version": "0.0.1",
  "type": "module",
  "description": "Fork of eslint-import-resolver-typescript",
  "repository": "git+https://github.com/RobinTail/eslint-import-resolver-typescript/tree/master",
  "author": "Alex Gorbatchev <alex.gorbatchev@gmail.com>",
  "contributors": [
    "JounQin (https://www.1stG.me) <admin@1stg.me>"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=18"
  },
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "shim.d.ts",
    "!**/*.tsbuildinfo"
  ],
  "keywords": [
    "typescript",
    "eslint",
    "import",
    "resolver",
    "plugin"
  ],
  "scripts": {
    "build": "run-p 'build:*'",
    "build:r": "r -f cjs,es2020",
    "build:ts": "tsc -b",
    "lint": "run-p 'lint:*'",
    "lint:es": "eslint src --cache -f friendly",
    "lint:tsc": "tsc --noEmit",
    "test": "run-p 'test:*'",
    "test:multipleEslintrcs": "eslint --ext ts,tsx tests/multipleEslintrcs",
    "test:multipleTsconfigs": "eslint --ext ts,tsx tests/multipleTsconfigs",
    "test:withJsExtension": "node tests/withJsExtension/test.js && eslint --ext ts,tsx tests/withJsExtension",
    "test:withJsconfig": "eslint --ext js tests/withJsconfig",
    "test:withPaths": "eslint --ext ts,tsx tests/withPaths",
    "test:withPathsAndNestedBaseUrl": "eslint --ext ts,tsx tests/withPathsAndNestedBaseUrl",
    "test:withQuerystring": "eslint --ext ts,tsx tests/withQuerystring",
    "test:withoutPaths": "eslint --ext ts,tsx tests/withoutPaths"
  },
  "peerDependencies": {
    "eslint": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-import-x": "*"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-import": {
      "optional": true
    },
    "eslint-plugin-import-x": {
      "optional": true
    }
  },
  "dependencies": {
    "debug": "^4.3.5",
    "enhanced-resolve": "^5.15.0",
    "eslint-module-utils": "^2.8.1",
    "fast-glob": "^3.3.2",
    "get-tsconfig": "^4.7.5",
    "is-bun-module": "^1.0.2",
    "is-glob": "^4.0.3"
  },
  "devDependencies": {
    "@mozilla/glean": "^3.0.0",
    "@pkgr/rollup": "^4.1.3",
    "@types/debug": "^4.1.12",
    "@types/is-glob": "^4.0.4",
    "@types/node": "^18.19.39",
    "@types/unist": "^2.0.10",
    "dummy.js": "link:dummy.js",
    "eslint": "^8.57.0",
    "@robintail/import-resolver": "link:.",
    "eslint-plugin-import": "npm:eslint-plugin-i@^2.29.1",
    "npm-run-all2": "^5.0.2",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "typescript": "^5.3.2"
  },
  "resolutions": {
    "@robintail/import-resolver": "link:.",
    "prettier": "^2.8.8"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
